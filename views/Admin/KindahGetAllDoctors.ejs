<script id="doctorProfile-template" type="text/html">

     <div class="kanban-sidebar" style="overflow: auto;">
                    <div class="card shadow-none quill-wrapper">
                        <div
                            class="card-header d-flex justify-content-between align-items-center border-

bottom px-2 py-1 side-card-title">
                            <h3 class="card-title side-card-title">
                                <b>Doctor Profile</b>
                            </h3>
                            <button type="button" class="close close-icon" onclick="viewDocProfile()">
                                <i class="bx bx-x"></i>
                            </button>
                        </div>
                        <!-- form start -->

                        <div class="card-content side-bar">
                            <div class="card-body slide-card">
                                    <div class="media" style="flex-wrap: wrap" >
                                       <div class="pr-50 doc-img" style="margin-left:10px, padding:0px !

impportant">
                                       {{#ProfilePicture}}
                                       <img src="{{ProfilePicture}}" alt="avtar img holder">                

      
                                        {{/ProfilePicture}}
                                        {{^ProfilePicture}}
                                        <img src="/assets/images/maledoc.png" alt="avtar img holder">       

               
                                       {{/ProfilePicture}}                             
                                       </div>
                                        <div>
                                            <h5 class="media-heading"  style="margin-left:10px">
                                                <br />
                                                <b>{{FullName}}</b>
                                            </h5>
                                            <div>
                                                <p> <b>Specialization</b>: {{Specialization}}</p>
                                            </div>
                                        </div>
                                </div>
                                <br />
                                <div class="row">
                                    <div class="col-md-6">
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bx-envelope icon-margin"></i>Email 

Address</span>:
                                            {{Email}}
                                        </p>
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bx-phone icon-margin"></i>Contact 

Number</span>:
                                            {{PhoneNumber}}
                                        </p>
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bxs-calendar icon-margin"></i>Date of 

Birth</span>: abc
                                        </p>
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bx-male icon-margin"></i>Gender</span>:

{{Gender}} </p>
                                    </div>
                                    <div class="col-md-6">
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bx-navigation icon-

margin"></i>Address</span>:{{Address}}</p>
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bx-globe-alt icon-

margin"></i>Country</span>:{{Country}} </p>
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bx-map icon-margin"></i>City</span>: {{City}} 

</p>
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bxs-map-pin icon-margin"></i>State</span>: 

{{State}} </p>
                                    </div>
                                </div>

                                <br />
                                <h5 class="no-color">Clinic Information</h5>
                                <div class="row">
                                    <div class="col-md-6">
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bx-building icon-margin"></i>Clinic 

Name</span>: {{ClinicName}}
                                        </p>
                                        <p> <span class="editprofile-icons"><i class="bx bx-map icon-

margin"></i>Clinic
                                                Address</span>: {{ClinicAddress}} </p>
                                    </div>
                                </div>

                                <br />
                                <h5 class="no-color">Education </h5>
                                <div class="row">
                                    <div class="col-md-6 col-12">
                                        {{#DoctorEducationsModel}}
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bxs-file-plus icon-margin"></i>Degree</span>:
                                                    {{Degree}}
                                           </p>
                                           {{/DoctorEducationsModel}}
                                            
                                            {{#DoctorEducationsModel}}
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bx-calendar-event icon-margin"></i>Year of
                                                Completion</span>: {{YearsOfCompletion}}
                                           </p>
                                            {{/DoctorEducationsModel}}
                                    </div>
                                </div>

                                <br />
                                <h5 class="no-color">Experience</h5>
                                <div class="row">
                                    <div class=" col-12">
                                        {{#DoctorExperianceModel}} 
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bx-buildings icon-

margin"></i>Hospital</span>:
                                                    {{HospitalName}}
                                      </p>
                                        {{/DoctorExperianceModel}} 

                                        {{#DoctorExperianceModel}} 
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bxs-folder-plus icon-

margin"></i>Designation</span>:
                                                    {{Designation}}
                                             </p>
                                               {{/DoctorExperianceModel}} 
                                                 {{#DoctorExperianceModel}} 
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bx-calendar-event icon-margin"></i>Year
                                            </span>: From {{From}} To {{To}}
                                          {{/DoctorExperianceModel}} </p>
                                    </div>
                                </div>

                                <br />
                                <h5 class="no-color">Awards</h5>
                                <div class="row">
                                    <div class="col-md-6">
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bx-trophy icon-margin"></i>Award Name</span>:
                                            {{Award}} </p>
                                        <p> <span class="editprofile-icons"><i
                                                    class="bx bxs-calendar-plus icon-

margin"></i>Year</span>:
                                           {{AwardYear}}  </p>

                                    </div>
                                </div>
                                
                                    <button type="button" class="btn btn-light-secondary" 

onclick="viewDocProfile()"
                                        style="float: right;">
                                        <span class="d-none d-sm-block">Close</span>
                                    </button>
                               <br /><br /><br /><br /><br />  
                            </div>
                        </div>
                    </div>
                </div>
</script>

